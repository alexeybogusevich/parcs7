{
  "AllowedHosts": "*",
  "Elasticsearch": {
    "BaseUrl": "http://elasticsearch:9200"
  },
  "Host": {
    "Uri": "parcs.hostapi",
    "GetModuleEndpoint": "api/modules/{0}",
    "GetModulesEndpoint": "api/modules",
    "PostModulesEndpoint": "api/modules",
    "DeleteModulesEndpoint": "api/modules/{0}",
    "GetJobEndpoint": "api/jobs/{0}",
    "GetJobOutputEndpoint": "api/jobOutputs/{0}",
    "GetJobsEndpoint": "api/jobs",
    "PostJobsEndpoint": "api/jobs",
    "PostCloneJobsEndpoint": "api/jobs/{0}",
    "PutJobEndpoint": "api/jobs/{0}",
    "PostAsynchronousRunsEndpoint": "api/asynchronousJobRuns"
  },
  "Portal": {
    "Uri": "parcs.portal",
    "SignalrUri": "parcs.portal",
    "JobCompletionEndpoint": "api/completedJobs/{0}"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "Properties": {
      "Application": "Parcs.Portal"
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "source": "csharp"
        }
      }
    ]
  }
}